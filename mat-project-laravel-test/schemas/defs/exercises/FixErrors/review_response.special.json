{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "exerType": {
            "type": "string",
            "const": "FixErrors"
        },
        "content": {
            "type": "array",
            "minItems":1,
            "items":{
                "anyOf":[
                    {
                        "type":"string"
                    },
                    {
                        "type":"object",
                        "title": "Insert action",
                        "properties": {
                            "INS":{
                                "type":"string"
                            }
                        },
                        "required": ["INS"]
                    },
                    {
                        "type":"object",
                        "title": "Delete action",
                        "properties": {
                            "DEL":{
                                "type":"string"
                            }
                        },
                        "required": ["DEL"]
                    }
                ]
            }
        }
    },
    "required": [
        "exerType",
        "content"
    ]
}