{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "List tasks response",
    "description": "Represents a list of tasks matching requested filters.",
    "properties": {
        "tasks": {
            "type": "array",
            "items": {
                "$ref":"@/defs/types/task/preview_info.json"
            }
        },
        "config":{
            "type":"object",
            "properties": {
                "prev_cursor":{
                    "type":"string"
                },
                "next_cursor":{
                    "type":"string"
                }
            }
        }
    },
    "required": ["tasks", "config"]
}